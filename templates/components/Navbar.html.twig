<div{{attributes}} class="navbar bg-base-100">
	<div class="flex-1">
		<a class="font-bold text-3xl">Symfony UX</a>
	</div>
	<div class="flex-none gap-2">
		<ul class="menu menu-horizontal px-1 text-xl">
			<li>
				<a href="{{ path('home') }}">Home</a>
			</li>
			{# <li>
				<a href="{{ path('spacex') }}">SpaceX</a>
			</li>
			<li>
				<a href="{{ path('task_list') }}">Tasks</a>
			</li> #}
		</ul>

		<div class="dropdown dropdown-end">
			<div tabindex="0" role="button" class="btn btn-ghost btn-circle avatar">
				<div class="w-10 rounded-full">
					<img alt="Tailwind CSS Navbar component" src="{{ this.imageProfile }}"/>
				</div>
			</div>
			<ul tabindex="0" class="menu menu-sm dropdown-content bg-base-100 rounded-box z-[1] mt-3 w-52 p-2 shadow">
				<li>
					<a>
						{{ this.user }}
					</a>
				</li>
				<li>
					<a>Settings</a>
				</li>
				<li>
					<twig:Button color="neutral" size="sm" has_link="true" url="{{ path('_logout_main') }}">Logout</twig:Button>
				</li>
			</ul>
		</div>
	</div>
</div>
